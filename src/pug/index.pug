include ./_components/data
main.main.main__ui-section
  ._container._container_index
    h1.h1.title Проект: 
      =site.title
    hr
    ul
    each link in links
      if (link.name == 'UI KIT')
        hr
        li
          a(href=`${link.path}`)
            b= link.name
      else 
        li
          a(href=`${link.path}`)= link.name
